#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/grpc_tools_node_protoc_ts@4.1.5/node_modules/grpc_tools_node_protoc_ts/bin/node_modules:/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/grpc_tools_node_protoc_ts@4.1.5/node_modules/grpc_tools_node_protoc_ts/node_modules:/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/grpc_tools_node_protoc_ts@4.1.5/node_modules:/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/grpc_tools_node_protoc_ts@4.1.5/node_modules/grpc_tools_node_protoc_ts/bin/node_modules:/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/grpc_tools_node_protoc_ts@4.1.5/node_modules/grpc_tools_node_protoc_ts/node_modules:/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/grpc_tools_node_protoc_ts@4.1.5/node_modules:/Users/fullsnack/Documents/play/grpc-ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../grpc_tools_node_protoc_ts/bin/protoc-gen-ts" "$@"
else
  exec node  "$basedir/../grpc_tools_node_protoc_ts/bin/protoc-gen-ts" "$@"
fi
